// Library with exported types and functions

export type Point
    float x = 0.0
    float y = 0.0

export method distance(Point this) =>
    math.sqrt(this.x * this.x + this.y * this.y)

export add(a, b) => a + b

export enum Direction
    UP = "Up"
    DOWN = "Down"
    LEFT = "Left"
    RIGHT = "Right"
