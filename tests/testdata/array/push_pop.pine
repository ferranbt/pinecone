// Test array.push and array.pop

a = array.new_float(0)

// Push values
array.push(a, 10.0)
array.push(a, 20.0)
array.push(a, 30.0)

size1 = array.size(a)
log.info(size1)

// Pop value
val1 = array.pop(a)
log.info(val1)

size2 = array.size(a)
log.info(size2)

// Get remaining values
val2 = array.get(a, 0)
val3 = array.get(a, 1)
log.info(val2)
log.info(val3)

// Expected output:
// 3
// 30
// 2
// 10
// 20
