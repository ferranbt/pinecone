// Test UDT field access and modification

type Point
    float x
    float y

// Create instance
p = Point.new(100, 50.5)

// Access fields
x1 = p.x
y1 = p.y
log.info(x1)
log.info(y1)

// Modify fields
p.x := 200
p.y := 75.5

// Access modified fields
x2 = p.x
y2 = p.y
log.info(x2)
log.info(y2)

// Expected output:
// 100
// 50.5
// 200
// 75.5
